
{
  "expo": {
    // The name of the Expo project
    "name": "expense-tracker",
    "slug": "expense-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mhsaeed.expensetracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        // The background color of the adaptive icon
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mhsaeed.expensetracker"
    },
    "web": {
      "favicon": "./assets/expense-tracker.png"
    }
  }
}
// In your code snippet, the adaptiveIcon object is used to define an adaptive icon for Android
